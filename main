    <script>
        document.addEventListener('keydown', (event) => {
            if (event.key === 'Enter') {
                event.preventDefault();
            }
        });
        const radioButtons = document.querySelectorAll('input[type="radio"]:not([name="testing_new_products"]):not([name="friends_come_to_me"]):not([name="getting_news_from_traditional_outlets"]):not([name="need_help_when_interacting_new_products"]):not([name="agree_to_survey"]):not([name="livingarea"]):not([name="housing"]):not([name="housing_parking"]):not([name="vehicle_usage"])');

        radioButtons.forEach(function (radio) {
            radio.addEventListener('change', function () {
                window.scrollBy({ top: 450, behavior: 'smooth' });
            });
        });

        const myForm = document.getElementById("myForm");
        const loadingScreen = document.getElementById("loadingScreen");

        myForm.addEventListener("submit", (event) => {
            event.preventDefault();

            // Show loading screen with fading animation
            loadingScreen.style.opacity = "1";
            loadingScreen.style.visibility = "visible";

            // Delay the form submission for 2 seconds
            setTimeout(() => {
                myForm.submit();
            }, 2000); // 2000 milliseconds (2 seconds)
        });

        $(document).ready(function () {
            // Hide the buttons by default
            $("#start-button, #exit-button").hide();

            // Handle radio button change event
            $('input[name="agree_to_survey"]').on("change", function () {
                var selectedValue = $(this).val();

                if (selectedValue === "Yes I agree") {
                    $("#start-button").fadeIn(300);
                    $("#exit-button").fadeOut(300);
                } else if (selectedValue === "No I do not agree") {
                    $("#start-button").fadeOut(300);
                    $("#exit-button").fadeIn(300);
                }
            });

            // Handle radio button change event
            $('input[name="authorized_future_contact"]').on("change", function () {
                var selectedValue = $(this).val();

                if (selectedValue === "True") {
                    $("#submit-button").fadeIn(300);
                } else {
                    $("#submit-button").fadeOut(300);
                }
            });

           // Handle change event for all radio buttons with the name "children"
            $('input[name="children"]').on("change", function () {
                // Check if any radio button with the name "children" is selected
                if ($('input[name="children"]:checked').length > 0) {
                    $("#section-complete-component-1").fadeIn(300);
                } else {
                    $("#section-complete-component-1").fadeOut(300);
                }
            });
                // Handle change event for all radio buttons with the name "children"
            $('input[name="reason_to_reserve"]').on("change", function () {
                // Check if any radio button with the name "reason_to_reserve" is selected
                if ($('input[name="reason_to_reserve"]:checked').length > 0) {
                    $("#section-complete-component-2").fadeIn(300);
                } else {
                    $("#section-complete-component-2").fadeOut(300);
                }
            });
                // Handle change event for all radio buttons with the name "children"
            $('input[name="futuristic_design"]').on("change", function () {
                // Check if any radio button with the name "futuristic_design" is selected
                if ($('input[name="futuristic_design"]:checked').length > 0) {
                    $("#section-complete-component-3").fadeIn(300);
                } else {
                    $("#section-complete-component-3").fadeOut(300);
                }
            });

        });

        function scrollToTop() {
            window.scrollTo({
                top: 0,
                behavior: 'smooth' // This makes the scrolling smooth
            });
        }
        const scrollToTopButton = document.getElementById("scrollToTopButton");

        scrollToTopButton.addEventListener("click", scrollToTop);


    </script>

